        },
        "contentDensities": {
          "compact": true,
          "cozy": true
        },
        "models": {
          "": {
            "dataSource": "Products",
            "type": "sap.ui.model.json.JSONModel"
          }
        }
      }
    }
  },
  "groups": {
    "cf_group": {
      "identification": {
        "id": "cf_group",
