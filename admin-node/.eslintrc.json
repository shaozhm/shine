{
    "extends": "eslint:recommended",
    "rules": {
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "eqeqeq": ["error", "always"],
        "comma-dangle": ["error", "always"],
        "no-trailing-spaces": ["error"],
        "camelcase": ["error", { "properties": "never" }]
    }
}