{
  "welcomeFile": "launchpad/index.html",
  "logout":  {
      "logoutEndpoint":  "/logout"
  },
  "routes": [
	{
      "source": "/sap/ui5/1(.*)",
      "target": "$1",
      "localDir": "sapui5"
    },
    {
      "source": "/sap/hana/democontent/epm/services/",
      "destination": "xsjs-backend"
    },
    {
      "source": "/get",
      "destination": "admin-backend",
      "scope": "shine-admin.DataGenerator"
    },
    {
      "source": "/reset",
      "destination": "admin-backend",
      "scope": "shine-admin.DataGenerator"
    },
    {
      "source": "/replicate",
      "destination": "admin-backend",
      "scope": "shine-admin.DataGenerator"
    },
    {
      "source": "^/admin-ui/(.*)",
      "localDir": "resources",
      "scope": "shine-admin.DataGenerator"
    },
    {
      "source": "^/(.*)",
      "localDir": "resources"
    }
  ]
}