{
	"welcomeFile": "launchpad/index.html",
	"logout": {
		"logoutEndpoint": "/logout"
	},
	"routes": [{
      		"source": "/sap/ui5/1(.*)",
      		"target": "$1",
      		"localDir": "sapui5",
		"cacheControl": "public, max-age=1000,must-revalidate"
    	}, {
		"source": "/sap/hana/democontent/epm/services/",
		"destination": "xsjs-backend"
	}, {
		"source": "/get/(.*)",
		"destination": "admin-backend",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "/reset/(.*)",
		"destination": "admin-backend",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "/replicate/(.*)",
		"destination": "admin-backend",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "/jobs/(.*)",
		"destination": "jobs-backend"
	},{
		"source": "/schedules/(.*)",
		"destination": "jobs-backend"
	},{
		"source": "/jobactivity/(.*)",
		"destination": "jobs-backend",
		"authenticationType":"basic"
	},{
		"source": "^/admin-ui/(.*)",
		"localDir": "resources",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "^/(.*)",
		"localDir": "resources",
		"cacheControl": "public, max-age=1000,must-revalidate"
	}]
} 
