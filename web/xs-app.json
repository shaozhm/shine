{
  "welcomeFile": "launchpad/index.html",
  "authenticationMethod": "route",
  "websockets": {
    "enabled": true
  },
  "logout": {
    "logoutEndpoint": "/logout",
    "logoutPage": "/"
  },
  "routes": [
	{
      "source": "^/python/download/(.*)$",
      "target": "$1",
      "destination": "core-python-backend"
    },
    {
      "source": "/node(.*)",
      "destination": "core-node-backend",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "/sap/bc/lrep(.*)",
      "destination": "core-node-backend",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "/sap/hana/democontent/epm/(.*)",
      "destination": "core-xsjs-backend"
    },
    {
      "source": "/sap/rest/(.*)",
      "destination": "uaa"
    },
    {
      "source": "/user/odata/v4/(.*)",
      "destination": "user-java-backend"
      
    },
    {
      "source": "/get/(.*)",
      "destination": "core-node-backend",
      "scope": "$XSAPPNAME.DataGenerator"
    },
    {
      "source": "/reset/(.*)",
      "destination": "core-node-backend",
      "scope": "$XSAPPNAME.DataGenerator"
    },
    {
      "source": "/replicate/(.*)",
      "destination": "core-node-backend",
      "scope": "$XSAPPNAME.DataGenerator"
    },
    {
      "source": "/jobs/(.*)",
      "destination": "core-node-backend",
      "scope": "$XSAPPNAME.JOBSCHEDULER"
      
    },
    {
      "source": "/schedules/(.*)",
      "destination": "core-node-backend",
      "scope": "$XSAPPNAME.JOBSCHEDULER"
    },
    {
      "source": "/jobactivity/(.*)",
      "destination": "core-node-backend",
      "authenticationType": "none"
    },
    {
      "source": "/user/(.*)", 
      "destination": "user-backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/admin-ui/(.*)",
      "localDir": "resources",
      "scope": "$XSAPPNAME.DataGenerator",
      "replace": {
        "pathSuffixes": [
          "index.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    },
       {
      "source": "^/job-ui/(.*)",
      "localDir": "resources",
      "scope": "$XSAPPNAME.JOBSCHEDULER",
      "replace": {
        "pathSuffixes": [
          "index.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    },
    {
      "source": "(.*)(.xsjs)",
      "destination": "core-xsjs-backend",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(.*)(.xsodata)",
      "destination": "core-xsjs-backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "/(.*)",
      "localDir": "resources",
      "authenticationType": "xsuaa",
      "cacheControl": "public, max-age=1000,must-revalidate",
      "replace": {
        "pathSuffixes": [
          "index.html",
          "odataTest.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    }
  ]
}
