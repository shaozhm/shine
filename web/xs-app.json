{
	"welcomeFile": "launchpad/index.html",
	"logout": {
		"logoutEndpoint": "/logout",
		"logoutPage": "/"
	},
	"routes": [ {
      	"source": "/sap/ui5/1(.*)",
      	"target": "$1",
      	"localDir": "sapui5"
    },{
		"source": "/sap/rest/(.*)",
		"destination": "uaa"
	}, {
		"source": "/sap/hana/democontent/epm/services/",
		"destination": "core-backend"
	}, {
		"source": "/get/(.*)",
		"destination": "core-backend",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "/reset/(.*)",
		"destination": "core-backend",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "/replicate/(.*)",
		"destination": "core-backend",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "/jobs/(.*)",
		"destination": "user-backend"
	}, {
		"source": "/schedules/(.*)",
		"destination": "user-backend"
	}, {
		"source": "/jobactivity/(.*)",
		"destination": "user-backend",
		"authenticationType": "basic"
	},{
		"source": "/user/xsodata/(.*)",
		"destination": "user-backend"
	}, {
		"source": "^/admin-ui/(.*)",
		"localDir": "resources",
		"scope": "$XSAPPNAME.DataGenerator"
	}, {
		"source": "^/user/odata/v4/(.*)",
		"destination": "user-java-backend"
	}, {
		"source": "^/(.*)",
		"localDir": "resources"
	}]
}