{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "websockets": {
    "enabled": true
  },
  "logout": {
    "logoutEndpoint": "/logout",
    "logoutPage": "/"
  },
  "routes": [
    {
      "source": "/node(.*)",
      "destination": "core-node-backend",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "/sap/bc/lrep(.*)",
      "destination": "core-node-backend",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "/sap/hana/democontent/epm/(.*)",
      "destination": "core-xsjs-backend"
    },
    {
      "source": "/user/odata/v4/(.*)",
      "destination": "user-java-backend"
      
    },
    {
      "source": "/get/(.*)",
      "destination": "admin-backend"
    },
    {
      "source": "/reset/(.*)",
      "destination": "admin-backend"
    },
    {
      "source": "/replicate/(.*)",
      "destination": "admin-backend"
    },
    {
      "source": "/jobs/(.*)",
      "destination": "admin-backend"
    },
    {
      "source": "/schedules/(.*)",
      "destination": "admin-backend"
    },
    {
      "source": "/jobactivity/(.*)",
      "destination": "admin-backend",
      "authenticationType": "basic"
    },
    {
      "source": "/user/(.*)", 
      "destination": "user-backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/admin-ui/(.*)",
      "localDir": "resources",
      
      "replace": {
        "pathSuffixes": [
          "index.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    },
    {
      "source": "(.*)(.xsjs)",
      "destination": "core-xsjs-backend",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(.*)(.xsodata)",
      "destination": "core-xsjs-backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "/(.*)",
      "localDir": "resources",
      "authenticationType": "xsuaa",
      
      "replace": {
        "pathSuffixes": [
          "index.html",
          "odataTest.html"
        ],
        "vars": [
          "sapui5url"
        ]
      }
    }
  ]
}