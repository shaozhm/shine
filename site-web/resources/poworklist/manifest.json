{
	"_version": "1.5.0",
	"sap.app": {
		"id": "shine.democontent.epm.poworklist",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": []
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/hana/democontent/epm/services/poExt.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": [
						"localAnnotations_1"
					]
				}
			},
			"localAnnotations_1": {
				"uri": "annotations/localAnnotations_1.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/localAnnotations_1.xml"
				}
			},
			"businessPartners.xsodata": {
				"uri": "/sap/hana/democontent/epm/services/businessPartners.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/businessPartners.xsodata/metadata.xml"
				}
			},
			"productDetails.xsodata": {
				"uri": "/sap/hana/democontent/epm/services/productDetails.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/productDetails.xsodata/metadata.xml"
				}
			}
		},
		"offline": false,
		"sourceTemplate": {
			"id": "ui5template.smarttemplate",
			"version": "1.45.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"resources": {
			"js": [],
			"css": []
		},
		"dependencies": {
			"minUI5Version": "1.45.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.suite.ui.generic.template": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {}
			},
			"components": {}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ListReport|POHeader": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ListReport/POHeader/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ObjectPage|POHeader": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ObjectPage/POHeader/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ObjectPage|POItem": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ObjectPage/POItem/i18n.properties"
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			},
			"BusinessPartnersModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "businessPartners.xsodata",
				"preload": true
			},
			"ProductDetailsModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "productDetails.xsodata",
				"preload": true
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"controllerName": "shine.democontent.epm.poworklist.ext.controller.Reports"
					},
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"controllerName": "shine.democontent.epm.poworklist.ext.controller.ListReportExt",
						"sap.ui.generic.app": {
							"POHeader": {
								"EntitySet": "POHeader",
								"Actions": {
									"ActionPOHeader1": {
										"id": "ActionPOHeader1button",
										"text": "{@i18n>ActionPOHeader1}",
										"press": "new"
									},
									"ActionPOHeader2": {
										"id": "ActionPOHeader2button",
										"text": "{@i18n>ActionPOHeader2}",
										"press": "delete",
										"icon":"sap-icon://delete"
									},
									"ActionPOHeader3": {
										"id": "ActionPOHeader3button",
										"text": "{@i18n>ActionPOHeader3}",
										"press": "exportExcel"
									},
									"ActionPOHeader4": {
										"id": "ActionPOHeader4button",
										"text": "{@i18n>ActionPOHeader4}",
										"press": "exportZip"
									},
									"ActionPOHeader5": {
										"id": "ActionPOHeader5button",
										"text": "{@i18n>ActionPOHeader5}",
										"press": "accept"
									},
									"ActionPOHeader6": {
										"id": "ActionPOHeader6button",
										"text": "{@i18n>ActionPOHeader6}",
										"press": "reject"
									}
								}
							}
						}
					}
				},
				"sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"SmartFilterBarControlConfigurationExtension|POHeader": {
							"className": "sap.ui.core.Fragment",
							"fragmentName": "shine.democontent.epm.poworklist.ext.fragment.Customfilter",
							"type": "XML"
						}
					},
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"AfterFacet|POHeader|ITEMS": {
							"type": "XML",
							"className": "sap.ui.core.Fragment",
							"fragmentName": "shine.democontent.epm.poworklist.ext.fragment.reports",
							"sap.ui.generic.app": {
								"title": "{@i18n>REPORTS}"
							}
						}
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ui.generic.app": {
		"pages": [
			{
				"entitySet": "POHeader",
				"component": {
					"name": "sap.suite.ui.generic.template.ListReport",
					"list": true,
					"settings":{
						
	                }
				},
				"pages": [
					{
						"entitySet": "POHeader",
						"component": {
							"name": "sap.suite.ui.generic.template.ObjectPage"
						}
					}
				]
			}
		]
	},
	"sap.platform.hcp": {
		"uri": ""
	}
}
