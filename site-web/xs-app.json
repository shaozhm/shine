{
	"routes": [{
		"source": "^/user-ui/(.*)",
		"target": "$1",
		"localDir": "resources/user-ui/"
	},{
		"source": "/user/xsodata/(.*)",
		"destination": "user-backend"
	},{
		"source": "^/user/odata/v4/(.*)",
		"destination": "user-java-backend"
	}, {
		"source": "^/(.*)",
		"localDir": "resources"
	}]
}